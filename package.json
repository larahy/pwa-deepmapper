    "webpack-dashboard": "^1.1.1",
    "webpack-dev-server": "^2.11.3",
    "webpack-merge": "^4.1.4",
    "copy-webpack-plugin": "^4.6.0",
    "workbox-webpack-plugin": "^3.6.3"
  },
